{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Product",
    "description": "A product in the catalog",
    "type": "object",
    "properties": {
        "name": {
            "description": "Display name of model",
            "type": "string"
        },
        "format": {
            "description": "File format for input data",
            "type": "string",
            "enum": [
                "midi",
                "musicxml"
            ]
        },
        "parameters": {
            "description": "Variables used in the run commands.",
            "type": "object",
            "items": {
                "type": [
                    "object",
                    "string"
                ],
                "properties": {
                    "min": {
                        "type": "number"
                    },
                    "max": {
                        "type": "number"
                    },
                    "enum": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "default": {},
                    "private": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "name"
                ]
            }
        },
        "script:": {
            "description": "Array of commands to run",
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "name",
        "format",
        "script"
    ]
}